@{
    @model User
}
<form method="POST" novalidate>
    <div class="form-group">
        @Html.EditorFor(m => m.Email)
    </div>
    <div class="form-group">
        @Html.EditorFor(m => m.FirstName)
    </div>
    <div class="form-group">
        @Html.EditorFor(m => m.LastName)
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>
